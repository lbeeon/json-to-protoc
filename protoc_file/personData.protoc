message PersonData {
  string kind = 1;
  string gender = 2;
  int64 age = 3;
  repeated CitiesLived citiesLived = 4;
  PhoneNumber phoneNumber = 5;
  string fullName = 6;
  repeated Children children = 7;
}

message Children {
  string gender = 1;
  string age = 2;
  string name = 3;
}

message PhoneNumber {
  string areaCode = 1;
  string number = 2;
}

message CitiesLived {
  repeated string yearsLived = 1;
  string place = 2;
}

